
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Siswas {
  id Int @id @default(autoincrement())
  kode_siswa String @unique
  nama_siswa String
  alamat String
  tanggal_lahir DateTime @db.Date
  jurusan String
}

